<div class="modal-header">
	<a data-dismiss="modal" class="close"></a>
	<h3 class="title-modal">Seguridad</h3>
</div>

<div class="modal-body">
	<div class="col-xs-12 column"> 
		<div class="row clearfix">  
			<div class="col-md-11 column">
				<div class="col-md-1 column input-group small-search">
					<input id="searchUser" type="text" class="form-control" style="float: left;width: 80%;">	  
					<div class="more wrapper-btn"><div class="btn-search-primary" onClick="searchUsers();"></div></div>
				</div>
			</div>				
			<div id="searchUserResult" class="col-md-12 column">
				<div id="securityUsers" class="col-md-6 column  table-with-border" style="float: left;"></div>
				<div id="securitySelected" class="col-md-6 column  table-with-border" style="float: right;">
					${data}
				</div>
			</div>				
		</div>
	</div>
</div>

  
