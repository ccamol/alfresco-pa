	<!-- Cron integración SAP -->
	<bean id="sapJobBean" class="org.springframework.scheduling.quartz.JobDetailBean">
		<property name="jobClass"
			value="com.intellego.parquearauco.quartz.sap.SapJobBean" />
		<property name="jobDataAsMap">
			<map>
				<entry key="authenticationComponent" value-ref="authenticationComponent" />
			</map>
		</property>
	</bean>
	<bean id="sapJobBeanTrigger" class="org.springframework.scheduling.quartz.CronTriggerBean">
		<property name="jobDetail" ref="sapJobBean" />
		<property name="cronExpression" value="0 0/1 * * * ?" />
	</bean>

	<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
		<property name="triggers">
			<list>
				<ref bean="sapJobBeanTrigger" />
			</list>
		</property>
	</bean>
	
