package com.intellego.parquearauco.dto;

import com.intellego.parquearauco.security.annotations.Dto;
import com.intellego.parquearauco.security.dto.Basic;

@Dto(entity = "com.intellego.parquearauco.entities.MallEntity")
public class Mall extends Basic{

	private static final long serialVersionUID = 1L;
	
	private String name;
	private String idMallSap;

	private String financeCompany;
	private String subProjectManager;
	private String imageUuid;
	private Country country;	
	private String nameImage;

	private MallType mallType;

	public Mall(){
	}

	public String getNameImage() {
		return nameImage;
	}

	public void setNameImage(String nameImage) {
		this.nameImage = nameImage;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getIdMallSap() {
		return idMallSap;
	}

	public void setIdMallSap(String idMallSap) {
		this.idMallSap = idMallSap;
	}

	public String getFinanceCompany() {
		return financeCompany;
	}

	public void setFinanceCompany(String financeCompany) {
		this.financeCompany = financeCompany;
	}

	public String getSubProjectManager() {
		return subProjectManager;
	}

	public void setSubProjectManager(String subProjectManager) {
		this.subProjectManager = subProjectManager;
	}


	public String getImageUuid() {
		return imageUuid;
	}

	public void setImageUuid(String imageUuid) {
		this.imageUuid = imageUuid;
	}

	public Country getCountry() {
		return country;
	}

	public void setCountry(Country country) {
		this.country = country;
	}

	public MallType getMallType() {
		return mallType;
	}

	public void setMallType(MallType mallType) {
		this.mallType = mallType;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}	
	
	
}
