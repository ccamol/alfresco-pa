package com.intellego.parquearauco.filters;

import com.intellego.parquearauco.security.filters.NumericFilter;
import com.intellego.parquearauco.security.filters.ObjectFilter;
import com.intellego.parquearauco.security.filters.TextFilter;

public class MallFilter extends ObjectFilter{

	private static final long serialVersionUID = 1L;
	private NumericFilter id;
	private TextFilter name;
	private TextFilter idMallSap;
	private TextFilter imageUuid;
	private TextFilter financeCompany;
	private TextFilter subProjectManager;
	private TextFilter nameImage;

	private NumericFilter country;	

	private NumericFilter mallType;

	public MallFilter(){
		this.id = new NumericFilter();
		this.id.setField("id");

		this.name = new TextFilter();
		this.name.setField("name");

		this.idMallSap = new TextFilter();
		this.idMallSap.setField("idMallSap");

		this.financeCompany = new TextFilter();
		this.financeCompany.setField("financeCompany");

		this.subProjectManager = new TextFilter();
		this.subProjectManager.setField("subProjectManager");

		this.country = new NumericFilter();
		this.country.setField("country");

		this.mallType = new NumericFilter();
		this.mallType.setField("mallType");
		
		this.imageUuid = new TextFilter();
		this.imageUuid.setField("imageUuid");
		
		this.nameImage = new TextFilter();
		this.nameImage.setField("nameImage");
		
		
	}
	
	
	
	public TextFilter getNameImage() {
		return nameImage;
	}



	public void setNameImage(TextFilter nameImage) {
		this.nameImage = nameImage;
	}



	public TextFilter getImageUuid() {
		return imageUuid;
	}



	public void setImageUuid(TextFilter imageUuid) {
		this.imageUuid = imageUuid;
	}



	public NumericFilter getId() {
		return id;
	}

	public void setId(NumericFilter id) {
		this.id = id;
	}

	public TextFilter getName() {
		return name;
	}

	public void setName(TextFilter name) {
		this.name = name;
	}

	public TextFilter getIdMallSap() {
		return idMallSap;
	}

	public void setIdMallSap(TextFilter idMallSap) {
		this.idMallSap = idMallSap;
	}

	public TextFilter getFinanceCompany() {
		return financeCompany;
	}

	public void setFinanceCompany(TextFilter financeCompany) {
		this.financeCompany = financeCompany;
	}

	public TextFilter getSubProjectManager() {
		return subProjectManager;
	}

	public void setSubProjectManager(TextFilter subProjectManager) {
		this.subProjectManager = subProjectManager;
	}

	public NumericFilter getCountry() {
		return country;
	}

	public void setCountry(NumericFilter country) {
		this.country = country;
	}

	public NumericFilter getMallType() {
		return mallType;
	}

	public void setMallType(NumericFilter mallType) {
		this.mallType = mallType;
	}
	
	
}
