	<!-- Notificaciones -->
	<!-- Documentos compartidos entre areas -->
	<bean id="sharedDocumentJobBean" class="org.springframework.scheduling.quartz.JobDetailBean">
		<property name="jobClass" value="com.intellego.pa.quartz.notification.NotificationSharedDocument" />
		<property name="jobDataAsMap">
			<map>
				<entry key="authenticationComponent" value-ref="authenticationComponent" />
				<entry key="serviceRegistry" value-ref="ServiceRegistry" />
				<entry key="namespaceService" value-ref="NamespaceService" />
				<entry key="nodeService" value-ref="NodeService" />
				<entry key="emailService" value-ref="emailService" />
				<entry key="properties" value-ref="global-properties" />
			</map>
		</property>
	</bean>
	<bean id="notifySharedDocumentJobBeanTrigger" class="org.springframework.scheduling.quartz.CronTriggerBean">
		<property name="jobDetail" ref="sharedDocumentJobBean" />
		<!-- 1 minuto -->
		<!-- <property name="cronExpression" value="0 * * * * ?" /> -->
		<property name="cronExpression" value="0 0 23 * * ?" />
	</bean>
	
	<!-- Documentos compartidos a terceros -->
	<bean id="notifyJobBean" class="org.springframework.scheduling.quartz.JobDetailBean">
		<property name="jobClass" value="com.intellego.pa.quartz.notification.NotificationSharedDocumentThird" />
		<property name="jobDataAsMap">
			<map>
				<entry key="authenticationComponent" value-ref="authenticationComponent" />
				<entry key="serviceRegistry" value-ref="ServiceRegistry" />
				<entry key="emailService" value-ref="emailService" />
				<entry key="properties" value-ref="global-properties" />
			</map>
		</property>
	</bean>
	<bean id="notifyJobBeanTrigger" class="org.springframework.scheduling.quartz.CronTriggerBean">
		<property name="jobDetail" ref="notifyJobBean" />
		<!-- 1 minuto -->
		 <!-- <property name="cronExpression" value="0 * * * * ?" /> -->
		<property name="cronExpression" value="0 0 23 * * ?" />
	</bean>

	<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
		<property name="triggers">
			<list>
				<ref bean="notifyJobBeanTrigger" />
				<ref bean="notifySharedDocumentJobBeanTrigger" />
				
			</list>
		</property>
	</bean>

