var uuid = (args.uuid == null) ? null : args.uuid;
var tenderId = args.tenderId ? args.tenderId : null;
var documentTypeId = args.documentTypeId ? args.documentTypeId : null;
var extension = args.extension ? args.extension : null;
var docName = args.docName ? args.docName : null;
var docRequiredId = args.docRequiredId ? args.docRequiredId : null;

if (extension.indexOf('.') == -1) {
	extension = "." + extension;
}

model.uuid = uuid;
model.tenderId = tenderId;
model.documentTypeId = documentTypeId;
model.extension = extension;
model.docName = docName;
model.docRequiredId = docRequiredId;