package com.intellego.parquearauco.entities;

import com.intellego.parquearauco.security.entities.BasicEntity;

public class CategoryTenderEntity extends BasicEntity{
	private Integer id;
	private TenderEntity tender;
	private CategoryEntity category;


	public CategoryTenderEntity(){
	}

	public CategoryTenderEntity(Integer id,TenderEntity tender,CategoryEntity category){
		this.id = id;
		this.tender = tender;
		this.category = category;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public TenderEntity getTender() {
		return tender;
	}

	public void setTender(TenderEntity tender) {
		this.tender = tender;
	}

	public CategoryEntity getCategory() {
		return category;
	}

	public void setCategory(CategoryEntity category) {
		this.category = category;
	}


}
