package com.intellego.parquearauco.security.entities;

import java.io.Serializable;

public class RolEntity extends BasicEntity implements Serializable{

	private static final long serialVersionUID = 1L;
	
	private Integer id;
	private String name;
	private String documentalRol;
	private String description;

	
	public RolEntity(){
		this.id=null;
		this.name=new String();
		this.documentalRol=new String();
	}
	
	public RolEntity(Integer id, String name, String documentalRol){
		this.id=id;
		this.name=name;
		this.documentalRol=documentalRol;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getDocumentalRol() {
		return documentalRol;
	}
	
	public void setDocumentalRol(String documentalRol) {
		this.documentalRol = documentalRol;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}
	
	

}
